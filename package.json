{
  "name": "eslint-plugin-rtl-friendly",
  "version": "0.3.1-1",
  "description": "Encourage the use of RTL-friendly styles",
  "author": {
    "name": "Ahmed Abdelbaset",
    "url": "https://twitter.com/A7med3bdulBaset"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "docs",
    "index.d.ts",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "type:check": "tsc --noEmit",
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "postclean": "rm -rf dist",
    "format": "biome check . --write",
    "lint": "eslint .",
    "format:check": "biome check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "release": "semantic-release",
    "add-rule": "tsx scripts/add-rule",
    "update": "tsx scripts/update",
    "prepublishOnly": "npm run build",
    "gen-e2e": "tsx scripts/generate-e2e"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "tailwindcss": ">=3.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@eslint/js": "9.5.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@types/eslint": "^8.56.10",
    "@types/estree": "^1.0.5",
    "@types/estree-jsx": "^1.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.5.4",
    "eslint": "^9.5.0",
    "eslint-plugin-eslint-plugin": "^6.1.0",
    "globals": "^15.3.0",
    "jest": "^29.7.0",
    "tailwindcss": "~3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "rtl",
    "right-to-left",
    "rtl-friendly"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AhmedBaset/eslint-plugin-rtl-friendly.git"
  },
  "bugs": {
    "url": "https://github.com/AhmedBaset/eslint-plugin-rtl-friendly/issues"
  },
  "homepage": "https://github.com/AhmedBaset/eslint-plugin-rtl-friendly#readme",
  "packageManager": "pnpm@9.4.0"
}
